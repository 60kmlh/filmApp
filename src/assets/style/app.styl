@import './vari'

*
  padding 0
  margin 0
  box-sizing border-box

body
  color #000

a
  text-decoration none
  color inherit

li
  list-style none
.tab_btn
  height 45px
  line-height 45px
  position fixed
  bottom 0
  width 100%
  background-color #fff
  border-top 1px solid border-color
.buy_btn
  font-size 12px
  display inline
  padding 4px 8px
  color red
  border 1px solid red
  border-radius 4px

